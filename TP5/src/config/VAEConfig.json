{
  "input": "data/emojis.txt",
  "pickle_input": "mlp_max_diff_0.pkl",
  "pickle_output": "mlp_max_diff_1.pkl",
  "input_length": 144,
  "encoder_hidden": [100, 50, 10],
  "decoder_hidden": [10, 50, 100],
  "activation": "SIGMOID",
  "beta": 0.7,
  "n": 0.01,
  "optimizer": "ADAM",
  "optimizer_config": {
    "adaptive_eta_iterations_increment": 20,
    "adaptive_eta_iterations_decrement": -20,
    "adaptive_eta_increment": 0.005,
    "adaptive_eta_decrement": 0.2,
    "adam_alpha": 0.001,
    "adam_beta1": 0.9,
    "adam_beta2": 0.999,
    "adam_epsilon": 1e-8,
    "momentum_beta": 0.05,
    "momentum_alpha": 0.9
  },
  "error": "QUADRATIC_MULTILAYER",
  "condition": "PIXEL_ERROR",
  "condition_config": {
    "epsilon": 0,
    "min_pixel_error": 1
  },
  "limit": 1000,
  "batch": 10,
  "test_pct": 0.8,
  "noise_stddev": 0.000001,
  "data_augmentation": false
}